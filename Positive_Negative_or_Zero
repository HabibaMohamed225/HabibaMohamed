#include <iostream>
using namespace std;
int main() {
     int num1,num2;
     cout<<"Enter number1:";
     cin>>num1;
     cout<<"Enter number2:";
     cin>>num2;
     if(num1 > num2){
        cout<<num1 <<" is greater than "<<num2;
     }
     else if(num2 > num1){
        cout<<num2 <<" is greater than "<<num1;
     }
     else{
        cout<<num1 <<" is equal "<<num2;
     }
    return 0;
}
